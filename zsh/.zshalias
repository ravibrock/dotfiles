# Homebrew
alias bbd="brew bundle dump --force --file=$DOTFILES/.brewfile"
alias bc="brew cleanup --prune=all"
alias bca="brew cleanup --prune=all && find "$(brew --prefix)/Caskroom" -type f '(' -name '*.dmg' -or -name '*.pkg' ')' -delete"
alias bcc="brew upgrade && bc"
alias bi="brew install"
alias bl="brew list"
alias blc="brew list --cask"
alias bo="brew outdated"
alias bu="brew upgrade"
alias bua="brew upgrade && brew upgrade --cask"
alias buag="brew upgrade && brew upgrade --cask --greedy"
alias buc="brew upgrade --cask"
alias bucg="brew upgrade --cask --greedy"
alias buh="brew upgrade --fetch-HEAD"
alias bx="brew uninstall"

# Cloudconvert
alias "clc"="cloudconvert convert -f"
alias "clm"="cloudconvert merge"
alias "clo"="cloudconvert optimize --overwrite"

# Filesystem
alias ..='cd ..'
alias ...='cd ../..'
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Git
alias "ga"="git add"
alias "ga*"="git add --all"
alias "gc"="git checkout"
alias "gcm"="git commit -m"
alias "gcmi"="git commit -m 'Initial commit'"
alias "gi"="git init"
alias "gi*"="gi; ga*; gcmi"
alias "gp"="git push"
alias "gpsom"="git push --set-upstream origin main"

# YT-DLP
alias "ytdl"="yt-dlp -S ext:mp4:m4a"
alias "ytdlf"="yt-dlp -f"
alias "ytdlF"="yt-dlp -F"
alias "ytdlt"="yt-dlp --skip-download --write-thumbnail --convert-thumbnails jpg"

# Miscellaneous
alias "clear"="clear; startup_printout; printf '\e[3J'"
alias "code"="codium"
alias "py"="python3"
alias "python"="python3"
alias "venvc"="virtualenv --activators bash venv"
alias "venvs"="source venv/bin/activate"
alias "vim"="nvim"
alias "wipe"="printf '\033c'; printf '\e[3J'; source ~/.zshrc"
